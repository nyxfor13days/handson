{
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": false,
  "singleAttributePerLine": false,
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.json, *.md",
      "options": {
        "printWidth": 1000
      }
    }
  ]
}
